<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tenderlist"
    android:background="@drawable/background">
    <Button
        android:id="@+id/notify"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/notify"

        android:textSize="10dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="290dp">

    </Button>
    <Button
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status"
        android:textStyle="bold"
        android:layout_below="@id/notify"
        android:layout_marginTop="10dp"
        android:layout_marginStart="260dp"
        android:textColor="@color/green_700"
        >
    </Button>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Government Tender List"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:layout_below="@id/status"
        android:layout_marginStart="90dp"
        android:textSize="20dp"
        >
    </TextView>
    <ScrollView

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="68dp"
        >
        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.cardview.widget.CardView
                android:id="@+id/card1"
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="23dp"
                app:cardElevation="15dp"
                android:layout_marginStart="30dp"

                app:layout_constraintVertical_bias="0.112"
                tools:ignore="MissingConstraints">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/text_written1"
                    android:textStyle="bold"
                    android:layout_marginStart="10dp"
                    android:textSize="17dp"
                    >
                </TextView>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="210dp"
                    android:layout_marginTop="105dp"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:text="view\nmore"
                    android:textColor="@color/white"
                    android:background="@color/green_700"
                    >

                </Button>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card2"
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="23dp"
                app:cardElevation="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                android:layout_below="@id/card1"
                app:layout_constraintVertical_bias="0.112"
                tools:ignore="MissingConstraints">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_written2"
                    android:textStyle="bold"
                    android:layout_marginStart="10dp"
                    android:textSize="17dp"
                    >

                </TextView>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="220dp"
                    android:layout_marginTop="108dp"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:text="view\nmore"
                    android:textColor="@color/white"
                    android:background="@color/green_700"
                    >

                </Button>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/card3"
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="23dp"
                app:cardElevation="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                android:layout_below="@+id/card2"
                app:layout_constraintVertical_bias="0.112"
                tools:ignore="MissingConstraints">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_written2"
                    android:textStyle="bold"
                    android:textSize="19dp"
                    >

                </TextView>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="250dp"
                    android:layout_marginTop="105dp"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:text="view\nmore"
                    android:textColor="@color/white"
                    android:background="@color/green_700"
                    >

                </Button>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/card4"
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="23dp"
                app:cardElevation="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                android:layout_below="@+id/card3"
                app:layout_constraintVertical_bias="0.112"
                tools:ignore="MissingConstraints">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_written2"
                    android:textStyle="bold"
                    android:textSize="17dp"
                    android:layout_marginStart="10dp"
                    >

                </TextView>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="220dp"
                    android:layout_marginTop="108dp"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:text="view\nmore"
                    android:textColor="@color/white"
                    android:background="@color/green_700"
                    >

                </Button>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/card5"
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="23dp"
                app:cardElevation="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="30dp"
                android:layout_below="@+id/card4"
                app:layout_constraintVertical_bias="0.112"
                tools:ignore="MissingConstraints">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_written2"
                    android:textStyle="bold"
                    android:textSize="17dp"
                    android:layout_marginStart="10dp"

                    >

                </TextView>
                <Button
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="220dp"
                    android:layout_marginTop="108dp"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:text="view\nmore"
                    android:textColor="@color/white"
                    android:background="@color/green_700"
                    >

                </Button>



            </androidx.cardview.widget.CardView>




        </RelativeLayout>


    </ScrollView>
    <ImageButton
        android:id="@+id/homeies"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="710dp"
        android:backgroundTint="@color/asmani"
        android:background="@drawable/hompeir"
        android:layout_marginStart="70dp"
        >

    </ImageButton>
    <ImageButton
        android:id="@+id/approval"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="710dp"
        android:backgroundTint="@color/asmani"
        android:background="@drawable/tick"
        android:layout_marginStart="140dp"
        >

    </ImageButton>
    <ImageButton
        android:id="@+id/cloud"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="710dp"
        android:backgroundTint="@color/asmani"
        android:background="@drawable/cloud"
        android:layout_marginStart="220dp"
        >

    </ImageButton>
    <ImageButton
        android:id="@+id/user"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="710dp"
        android:backgroundTint="@color/asmani"
        android:background="@drawable/user"
        android:layout_marginStart="290dp"
        >

    </ImageButton>

</RelativeLayout>